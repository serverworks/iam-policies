{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "NotAction": [
                "iam:*",
                "organizations:*",
                "aws-portal:*",
                "savingsplans:CreateSavingsPlans"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "iam:*ServerCertificate*",
                "iam:PassRole",
                "iam:GetRole",
                "iam:CreateRole",
                "iam:ListRoles",
                "iam:PutRolePolicy",
                "iam:CreateServiceLinkedRole",
                "iam:DeleteServiceLinkedRole"
            ],
            "Resource": "*"
        }
    ]
}
